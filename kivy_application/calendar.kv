# file: empty.kv

BoxLayout:
    orientation: 'vertical'
    padding: [10, 20, 10, 20]
    spacing: 15
    
    Button:
        text: 'This button does nothing'
        size_hint: None, None
        size: 200, 50
        on_press: pass  # Does nothing when pressed
        
    Label:
        text: 'This label does nothing'
        size_hint: None, None
        size: 300, 50
        color: 0, 0, 0, 1  # Black text
        
    TextInput:
        hint_text: 'This input does nothing'
        multiline: False
        size_hint: None, None
        size: 300, 40
        on_text_validate: pass  # Does nothing when the user presses enter
        
    BoxLayout:
        orientation: 'horizontal'
        
        Button:
            text: 'Inner Button 1'
            on_press: pass  # Does nothing
        
        Button:
            text: 'Inner Button 2'
            on_press: pass  # Does nothing
